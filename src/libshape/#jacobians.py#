import numpy as np
from typing import List,Tuple

from ..libutils.Point import Point



def jacobian1d(x:Tuple(Point,Point))->Tuple[np.ndarray,float]:
    '''
    returns the Jacobian determinant and Jacobian of a 1D linear element living in 3D space
    for a 1d element in 3D space, we can always change the variables to a single parameter
    which goes from 0 to L from the first node to the second, where L is the length of the element
    '''
    
    return (jacdet,jacdet)
